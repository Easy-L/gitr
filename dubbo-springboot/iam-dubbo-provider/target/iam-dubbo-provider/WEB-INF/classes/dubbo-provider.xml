<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans        
    http://www.springframework.org/schema/beans/spring-beans.xsd        
    http://code.alibabatech.com/schema/dubbo        
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <!-- 提供方应用信息，用于计算依赖关系 -->	
      <dubbo:application name="${iam.dubbo.application.name}"  />
       <!-- dubbo注册中心配置 -->
	  <dubbo:registry id="IAM-DUBBO-PROVIDER-REG"
		protocol="${iam.dubbo.register.protocol}" address="${iam.dubbo.register.url}"
		register="${iam.dubbo.register.register}"
		subscribe="${iam.dubbo.register.subscribe}" timeout="${iam.dubbo.register.timeout}"/>	
     	<!-- dubbo协议配置 -->
	  <dubbo:protocol name="${iam.dubbo.protocol.name}" port="${iam.dubbo.protocol.port}" />
	  <dubbo:provider timeout="5000"/>
	  
	  <bean id="demoService" class="com.zsgr.provider.service.impl.DemoServiceImpl" />
	  <dubbo:service interface="com.zsgr.api.service.DemoService"  ref="demoService" timeout="${iam.dubbo.service.timeout}" />
	 
	  <bean id="userService" class="com.zsgr.provider.service.impl.UserServiceImpl" />
	  <dubbo:service interface="com.zsgr.api.service.UserService"  ref="userService" timeout="${iam.dubbo.service.timeout}" />
				
    </beans>